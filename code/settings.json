{
    "editor.autoClosingQuotes": "never",
    "editor.autoIndent": "advanced",
    "editor.autoSurround": "never",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.links": false,
    "editor.mouseWheelZoom": true,
    "editor.rulers": [
        100
    ],
    "editor.roundedSelection": false,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.fontFamily": "'Fira Code', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.acceptSuggestionOnEnter": "off",
      // Controls whether suggestions should automatically show up while typing.
    "editor.quickSuggestions":
    {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.suggest.localityBonus": true,
    "extensions.ignoreRecommendations": false,
    "workbench.commandPalette.history": 500,
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.colorTheme": "Default High Contrast",
    "workbench.statusBar.feedback.visible": false,
    "workbench.tree.horizontalScrolling": true,
    "breadcrumbs.enabled": true,
    "breadcrumbs.filePath": "last",
    "workbench.editor.tabCloseButton": "off",
    "workbench.editor.tabSizing": "shrink",
    "update.enableWindowsBackgroundUpdates": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "search.useRipgrep": false,
    "editor.renderWhitespace": "trailing",
    "editor.wordWrap": "wordWrapColumn",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingBrackets": "false",
    "markdown.extension.toc.updateOnSave": false,
    "update.channel": "none",
    "files.insertFinalNewline": true,
    "pgsql.completion.enabled": false,
    "pgsql.signatureHelp.enabled": false,
    "typescript.format.placeOpenBraceOnNewLineForFunctions": true,
    "typescript.format.placeOpenBraceOnNewLineForControlBlocks": true,
    "html.autoClosingTags": false,
    "javascript.autoClosingTags": false,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
    "typescript.autoClosingTags": false,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
    "editor.accessibilitySupport": "off",
    "explorer.confirmDelete": false,
    "editor.autoClosingBrackets": "never",
    "debug.inlineValues": true,
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.toolBarLocation": "docked",
    "editor.cursorWidth": 3,
    "workbench.colorCustomizations": {
    "editorCursor.foreground": "#11aa11",
    "editorInlayHint.background": "#ffffff00",
    "editorInlayHint.foreground": "#808080",
    // "editorInlayHint.parameterBackground": "#ff0000",
    // "editorInlayHint.parameterForeground": "#ff0000",
    // "editorInlayHint.typeForeground": "#ff0000",
    // "editorInlayHint.typeBackground": "#ff0000"
    },
    "explorer.confirmDragAndDrop": false,
    "files.exclude": {
        ".history": true,
        ".vscode": true
    },
    "search.collapseResults": "alwaysCollapse",
    "search.exclude": {
        ".history": true,
        ".vscode": true
    },
    "editor.find.cursorMoveOnType": false,
    "editor.formatOnSaveMode": "modifications",
    "bookmarks.label.suggestion": "suggestWhenSelectedOrLineWhenNoSelected",
    "bookmarks.saveBookmarksInProject": true,
    "bookmarks.sideBar.expanded": true,
    "go.docsTool": "gogetdoc",
    "go.gopath": "",
    "lineHighlightBookmark.lineStyle": "dashed",
    "cSpell.userWords": [
        "Dendron",
        "Promscale",
        "denormalize",
        "elog",
        "jsonb",
        "quals"
    ],
    "security.workspace.trust.untrustedFiles": "open",
    "window.zoomLevel": 2,
    "editor.autoClosingOvertype": "never",
    "telemetry.telemetryLevel": "off",
    "rust-analyzer.inlayHints.maxLength": 32,
    "keyboard.dispatch": "keyCode",
    "editor.cursorBlinking": "phase",
    "editor.cursorStyle": "block",
    "editor.cursorSurroundingLines": 10,
    "gitlens.mode.active": "review",
    "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.expressionAdjustmentHints.hideOutsideUnsafe": true,
    "rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
    "rust-analyzer.inlayHints.typeHints.hideNamedConstructor": true,
    "rust-analyzer.joinLines.joinAssignments": false,
    "rust-analyzer.joinLines.joinElseIf": false,
    "rust-analyzer.joinLines.removeTrailingComma": false,
    "rust-analyzer.joinLines.unwrapTrivialBlock": false,
    "editor.inlayHints.padding": true,
    "rust-analyzer.inlayHints.expressionAdjustmentHints.enable": "always",
    "rust-analyzer.inlayHints.typeHints.hideClosureInitialization": true,
    "C_Cpp.intelliSenseEngine": "Tag Parser",
    "cSpell.enabled": false,
    "cSpell.overrides": []
}
